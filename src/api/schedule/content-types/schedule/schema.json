{
  "kind": "collectionType",
  "collectionName": "schedules",
  "info": {
    "singularName": "schedule",
    "pluralName": "schedules",
    "displayName": "Schedule"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "available": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "time": {
      "type": "time",
      "default": "14:30:00.000",
      "required": true
    },
    "timeEnd": {
      "type": "time",
      "default": "15:30:00.000",
      "required": true
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "schedule"
    }
  }
}
